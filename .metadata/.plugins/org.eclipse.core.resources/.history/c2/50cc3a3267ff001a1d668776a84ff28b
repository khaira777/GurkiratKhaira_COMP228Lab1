import java.util.Date;

public class Singers {
	int _id;
	String _name;
	String _address;
	Date _dob;
	int _noOfAlbumsPublished;

	public Singers() {

	}

	public Singers(int id) {
		_id = id;
	}

	public Singers(int id, String name) {
		_id = id;
		_name = name;
	}

	public Singers(int id, String name, String address) {
		_id = id;
		_name = name;
		_address = address;
	}

	public Singers(int id, String name, String address, Date dob) {
		_id = id;
		_name = name;
		_address = address;
		_dob = dob;
	}

	public Singers(int id, String name, String address, Date dob, int noOfAlbumsPublished) {
		_id = id;
		_name = name;
		_address = address;
		_dob = dob;
		_noOfAlbumsPublished = noOfAlbumsPublished;
	}

	public int getId() {
		return _id;
	}

	public void setId(int newId) {
		_id = newId;
	}

	public String getName() {
		return _name;
	}

	public void setName(String newName) {
		_name = newName;
	}

	public String getAddress() {
		return _address;
	}

	public void setAddress(String newAddress) {
		_address = newAddress;
	}

	public Date getDob() {
		return _dob;
	}

	public void setDob(Date newDob) {
		_dob = newDob;
	}
}
